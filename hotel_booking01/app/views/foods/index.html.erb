<% @foods.each do |food| %>
  
  <p><%= food.name %></p>
  <p><%= food.price %></p>
  <p><%= food.description %></p>
  <p><%= food.food_type.name %></p>
  <%= link_to 'View', food_path(food)  %>
  <%= link_to 'Edit', edit_food_path(food)  %>
  <%= button_to 'Delete', food_path(food), method: 'delete'  %>
<% end %>