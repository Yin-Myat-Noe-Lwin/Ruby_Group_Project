<% @room_types.each do |room_type| %>
  <%= image_tag(room_type.room_type_img, :alt => "img"  ) %>	
  <p><%= room_type.name %></p>
  <p><%= room_type.max_capacity %></p>
  <%= link_to 'View', room_type_path(room_type)  %>
  <%= link_to 'Edit', edit_room_type_path(room_type)  %>
  <%= button_to 'Delete', room_type_path(room_type), method: 'delete'  %>
<% end %>